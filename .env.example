OPENAI_API_KEY=sk-...
GEMINI_API_KEY=AIza...
GEMINI_FREE_API_KEY=AIza...
ANTHROPIC_API_KEY=sk-...
NVIDIA_API_KEY=nvapi-...
OPENROUTER_API_KEY=sk-...
XAI_API_KEY=xai-...

SAIMEMORY_DEBUG=true
LLM_PROVIDER=google
LLM_MODEL=gemini-2.5-flash

# Behavior
SAIMEMORY_TEMPERATURE=1
SAIMEMORY_SYSTEM_PROMPT=
SAIMEMORY_SYSTEM_PROMPT_FILE=./prompts/system.txt

# Memory
SAIMEMORY_DB_PATH=memory.db
SAIMEMORY_MEMORY=on
SAIMEMORY_MEMORY_LAST_MESSAGES=40
SAIMEMORY_MEMORY_SEMANTIC_RECALL=true
SAIMEMORY_MEMORY_TOPK=5
SAIMEMORY_MEMORY_RANGE_BEFORE=2
SAIMEMORY_MEMORY_RANGE_AFTER=2
SAIMEMORY_MEMORY_SCOPE=resource
SAIMEMORY_EMBED_MODEL=intfloat/multilingual-e5-large
SAIMEMORY_MEMORY_CHUNK_MIN_CHARS=120
SAIMEMORY_MEMORY_CHUNK_MAX_CHARS=480

SAIVERSE_RECALL_SNIPPET_MAX_CHARS=8000
SAIVERSE_RECALL_SNIPPET_STREAM_MAX_CHARS=8000
SAIVERSE_RECALL_SNIPPET_PULSE_MAX_CHARS=8000

# Summary
SAIMEMORY_SUMMARY=false
SAIMEMORY_SUMMARY_PRERUN=false
SAIMEMORY_SUMMARY_MAX_CHARS=4000
SAIMEMORY_SUMMARY_PROMPT_FILE=./prompts/summary-ja.tx

SAIVERSE_LOG_LEVEL=DEBUG

SDS_URL=http://127.0.0.1:8080

# Database Backup (saiverse.db)
# Automatically backup saiverse.db on startup (recommended: true)
SAIVERSE_DB_BACKUP_ON_START=true
# Number of backup copies to keep (default: 10)
SAIVERSE_DB_BACKUP_KEEP=10

# SAIMemory Backup (per-persona memory.db)
# Automatically backup persona memory.db on startup (recommended: true)
SAIMEMORY_BACKUP_ON_START=true
# rdiff-backup path (optional, auto-detected if not specified)
# SAIMEMORY_RDIFF_PATH=/usr/bin/rdiff-backup

# SEA
SAIVERSE_SEA_ENABLED=1

# SEA runtime debugging (optional)
# SAIVERSE_SEA_TRACE=1
# SAIVERSE_SEA_DUMP=temp/sea_dump.log

# Discord Gateway (optional)
SAIVERSE_GATEWAY_ENABLED=0
SAIVERSE_GATEWAY_WS_URL=wss://example.com/ws
SAIVERSE_GATEWAY_TOKEN=
SAIVERSE_GATEWAY_CHANNEL_MAP={}
SAIVERSE_GATEWAY_MEMORY_CHUNK_SIZE=65536

SAIVERSE_PULSE_MODEL=gemini-2.0-flash
SAIVERSE_GEMINI_ATTACHMENT_LIMIT=4
SAIVERSE_DISABLE_GEMINI_SSE_PATCH=0
SAIVERSE_DISABLE_GEMINI_STREAMING=1
SAIVERSE_LLM_CONTEXT_DUMP=llm_context.log

SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=yourmail@gmail.com
SMTP_PASSWORD=
SMTP_FROM="SAIVerse"
SMTP_USE_TLS=true

# SAIVERSE_DEFAULT_LIGHTWEIGHT_MODEL=qwen/qwen3-next-80b-a3b-instruct
# SAIVERSE_DEFAULT_MODEL=mistralai/mistral-large-3-675b-instruct-2512
# SAIVERSE_AGENTIC_MODEL=qwen/qwen3-coder-480b-a35b-instruct

MEMORY_WEAVE_MODEL=gemini-3-flash-preview
MEMORY_WEAVE_BATCH_SIZE=20
MEMORY_WEAVE_CONSOLIDATION_SIZE=10
MEMORY_WEAVE_MAINTAIN_INTERVAL=200

ENABLE_MEMORY_WEAVE_CONTEXT=false