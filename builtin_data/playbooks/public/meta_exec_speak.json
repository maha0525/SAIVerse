{
    "name": "meta_exec_speak",
    "description": "Execute a playbook and speak the result (used by call_playbook tool)",
    "input_schema": [
        {
            "name": "selected_playbook",
            "description": "Name of the playbook to execute"
        },
        {
            "name": "selected_args",
            "description": "Arguments for the playbook"
        }
    ],
    "output_schema": null,
    "router_callable": false,
    "user_selectable": false,
    "nodes": [
        {
            "id": "exec",
            "type": "exec",
            "action": null,
            "next": "finalize",
            "conditional_next": null
        },
        {
            "id": "finalize",
            "type": "subplay",
            "playbook": "sub_speak",
            "args": {"input": "{input}"},
            "propagate_output": true,
            "next": null
        }
    ],
    "start_node": "exec"
}
